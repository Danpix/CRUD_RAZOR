@model CrudRazorCore.Models.Asesor

<h1>Eliminar Asesor</h1>

<h3>¿Seguro que deseas eliminar este registro?</h3>

<dl class="row">
    <dt class="col-sm-2">Id</dt>
    <dd class="col-sm-10">@Model.Id_Asesor</dd>

    <dt class="col-sm-2">Nombre</dt>
    <dd class="col-sm-10">@Model.Nombre_Asesor</dd>

    <dt class="col-sm-2">Correo</dt>
    <dd class="col-sm-10">@Model.Correo_Asesor</dd>

    <dt class="col-sm-2">Puesto</dt>
    <dd class="col-sm-10">@Model.Puesto</dd>

    <dt class="col-sm-2">Teléfono</dt>
    <dd class="col-sm-10">@Model.Telefono_Asesor</dd>
</dl>

<form asp-action="Delete">
    <input type="hidden" asp-for="Id_Asesor" />
    <button type="submit" class="btn btn-danger">Eliminar</button>
    <a asp-action="Index" class="btn btn-secondary">Cancelar</a>
</form>
